{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Plain English",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.plain",
				"match": "\\b(a|about|an|and|another|are|at|bigger than|blank|bottom|break|call|called|divided by|exit|for|given|If|if|in|intel|into|is|left|less than|Loop|loop|minus|multiplyed by|nil|of|on|or|other|plus|Repeat|repeat|right|'s|some|the|then|To|to|top|using|was|with)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.plain",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.plain",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.plain"
}
