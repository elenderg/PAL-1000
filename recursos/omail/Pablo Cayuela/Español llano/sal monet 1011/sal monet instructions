      text -1 0 0 -1 0 0 1440 9000 3420 9360 0 "" "osmosian" 360 "center" 0 0 0 yes
         "dibujar"
      end
      text -1 0 0 -1 0 0 4320 9000 6660 9360 0 "" "osmosian" 360 "center" 0 0 0 yes
         "refrescar"
      end
      text -1 0 0 -1 0 0 1440 10440 6480 10800 0 "" "osmosian" 360 "center" 0 0 0 yes
         "mostrar"
      end
