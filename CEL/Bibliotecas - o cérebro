  Decodifique $660FB600. \ movzx eax,byte ptr [eax]
  Decodifique $8B9D0C000000. \ mov ebx,[ebp+12] ; ->  salva a word (ebp+12) no registrador ebx
  Decodifique $668903. \ mov [ebx],ah

