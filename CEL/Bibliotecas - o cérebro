  Decodifique $8B8D0C000000. \ mov ecx,[ebp+12] \ a segunda byte
  Decodifique $3A19. \ cmp bl,[ecx]
  Decodifique $0F8706000000. \ ja sobre o próximo 1 statement
  Decodifique $C7C000000000. \ mov eax,0 \ now it is false

