Intel $8B8508000000. \ mov eax,[ebp+8] \ o subtexto
Intel $8B9D0C000000. \ mov ebx,[ebp+12] \ o string
\ coloque o first deste string into o subtexto's first
Intel $8B8B00000000. \ mov ecx,[ebx+0] \ o string's first
Intel $898800000000. \ mov [eax+0],ecx \ o subtexto's first
\ coloque o last deste string into o last deste subtexto
Intel $8B8B04000000. \ mov ecx,[ebx+4] \ o string's last
Intel $898804000000. \ mov [eax+4],ecx \ o last deste subtexto

