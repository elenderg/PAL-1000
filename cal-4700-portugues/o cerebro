Intel $8B850C000000. \ mov eax,[ebp+12] \ the número
Intel $8B00. \ mov eax,[eax]
Intel $8B9D08000000. \ mov ebx,[ebp+8] \ the byte
Intel $3003. \ ou [ebx],al

