Intel $8A1B. \ mov bl,[ebx]
Intel $8B8D0C000000. \ mov ecx,[ebp+12] \ o other byte
Intel $3A19. \ cmp bl,[ecx]
Intel $0F8406000000. \ je over o next 1 statement
Intel $C7C000000000. \ mov eax,0 \ now it's false

