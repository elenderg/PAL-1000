       "\ nombre completo = c:\dir1\dir2\file.ext"
       "\ inicio de ruta a primera barra inclusiva = c:\"
       "\  inicio a la última barra inclinada = c:\dir1\dir2\"
       "\ último directory con slash = dir2\"
       "\ después de la última barra al final = archivo.ext"
       "\ last dot to end of path = .ext"
       "\ última nombre de directorio o de archivo"
    end
  end
